{
  "name": "svelte-template",
  "version": "1.0.0",
  "description": "Svelte template (with support for eslint, wallaby, prettier, and parcel)",
  "author": "Oscar Quinteros <mrtonyq22@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "jest",
    "start": "parcel index.html",
    "build": "npm run lint && npx prettier --write --plugin-search-dir=. ./**/*.svelte && parcel build --no-source-maps index.html robots.txt",
    "lint": "eslint --cache --ext .js src/",
    "lint:fix": "eslint --cache --ext .js --fix --rule \"no-debugger: 0\" src/",
    "server": "nodemon src/server.js"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@testing-library/svelte": "^1.11.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "chance": "^1.1.4",
    "chromedriver": "^77.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "jest": "^25.1.0",
    "jest-transform-svelte": "^2.1.0",
    "nightwatch": "^1.2.4",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-svelte": "^4.0.5",
    "prettier": "^1.19.1",
    "prettier-plugin-svelte": "^0.7.0",
    "serve": "^11.2.0",
    "svelte": "^3.20.1",
    "svelte-preprocess": "^3.3.0"
  },
  "dependencies": {
    "@beyonk/svelte-social-auth": "^2.0.0",
    "api.js": "0.0.5",
    "bulma": "^0.8.0",
    "cl-editor": "^2.0.0",
    "dropzone": "^5.7.0",
    "jwt-decode": "^2.2.0",
    "local-storage": "^2.0.0",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0",
    "prerender": "^5.6.0",
    "prerender-node": "^3.2.5",
    "qs": "^6.9.1",
    "regexparam": "^1.3.0",
    "sortablejs": "^1.10.2",
    "src": "^1.1.2",
    "svelte-spa-router": "^2.0.1",
    "svelte-swipe": "^1.5.0",
    "tools": "0.0.0",
    "utils": "^0.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mylastore/svelte-parcel.git"
  }
}
